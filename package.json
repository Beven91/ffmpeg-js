{
  "name": "ffmpeg-js",
  "version": "1.0.1",
  "description": "A lightweight ffmpeg webassembly",
  "types": "src/index.ts",
  "main": "dist/index.js",
  "keywords": [
    "ffmpeg",
    "ffmpeg-js",
    "webassembly",
    "wasm"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "start": "cd example && ts-node ./www",
    "asmlib": "babel --minified assembly/audio.js --out-file dist/audio.js --presets=@babel/preset-env",
    "build": "tsc && npm run asmlib  && node apply.js && npm run umd",
    "umd": "browserify --entry dist/src/index.js --noparse=$PWD/dist/assembly/audio.js  --no-bundle-external --ignore-missing  --outfile dist/umd/ffmpegjs.js",
    "dev": "watchify --no-bundle-external --entry src/index.ts --outfile dist/umd/ffmpegjs.js -p [ tsify --noImplicitAny ]",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@types/node": "^17.0.21",
    "node-web-mvc": "^2.13.9",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^8.10.2",
    "tsify": "^5.0.4",
    "typescript": "^4.3.5",
    "watchify": "^4.0.0",
    "@babel/preset-env":"^7.18.2"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
  }
}
